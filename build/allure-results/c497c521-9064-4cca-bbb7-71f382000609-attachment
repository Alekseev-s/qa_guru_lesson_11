<script>

$('#vacancy-form .vacancy-form-file-select').click(function(e) {
    e.preventDefault();
    console.log("click");
    if(!$("#vacancy-form .vacancy-form-file-select").val()) {
        $('#vacancy-form .input-file').trigger("click");
    }
});

$('#vacancy-form .input-file').on("change", function() {
    var filename = $('#vacancy-form .input-file').val().replace(/C:\\fakepath\\/i, '')
    $("#vacancy-form .vacancy-form-file-select").val(filename);
    $("#vacancy-form .file-delete-icon").fadeIn();
    $('#vacancy-form .input-file').valid();
});

$('#vacancy-form .file-delete-icon').click(function(e) {
    e.preventDefault();
    $("#vacancy-form .vacancy-form-file-select").val("");
    $('#vacancy-form .input-file').val("");
    $("#vacancy-form .file-delete-icon").fadeOut();
});

</script>
