<script>

$('#vacancy-form-main .vacancy-form-file-select').click(function(e) {
    console.log("click");
    e.preventDefault();
    if(!$("#vacancy-form-main .vacancy-form-file-select").val()) {
        console.log("trigger file dialog!");
        $('#vacancy-form-main .input-file').trigger("click");
    }
});

$('#vacancy-form-main .input-file').on("change", function() {
    var filename = $('#vacancy-form-main .input-file').val().replace(/C:\\fakepath\\/i, '')
    $("#vacancy-form-main .vacancy-form-file-select").val(filename);
    $("#vacancy-form-main .file-delete-icon").fadeIn();
    $('#vacancy-form-main .input-file').valid();
});

$('#vacancy-form-main .file-delete-icon').click(function(e) {
    e.preventDefault();
    $("#vacancy-form-main .vacancy-form-file-select").val("Добавьте файл");
    $('#vacancy-form-main .input-file').val("");
    $("#vacancy-form-main .file-delete-icon").fadeOut();
});

</script>



